{
    "kind": "Template",
    "apiVersion": "v1",
    "metadata": {
        "name": "mlbparks-pipeline",
        "creationTimestamp": null
    },
    "objects": [
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "mlbparks-pipeline",
                "creationTimestamp": null,
                "labels": {
                    "app": "mlbparks-pipeline",
                    "application": "mlbparks-pipeline",
                    "build": "mlbparks-pipeline",
                    "createdBy": "mlbparks-template"
                },
                "annotations": {
                    "pipeline.alpha.openshift.io/uses": "[{\"name\": \"mlbparks\", \"namespace\": \"\", \"kind\": \"DeploymentConfig\"}]"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "GitHub",
                        "github": {
                            "secret": "${GITHUB_TRIGGER_SECRET}"
                        }
                    },
                    {
                        "type": "Generic",
                        "generic": {
                            "secret": "${GENERIC_TRIGGER_SECRET}"
                        }
                    }
                ],
                "runPolicy": "Serial",
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/sspeiche/mlbparks.git",
                        "ref": "master"
                    }
                },
                "strategy": {
                    "type": "JenkinsPipeline",
                    "jenkinsPipelineStrategy": {
                        "jenkinsfilePath": "Jenkinsfile"
                    }
                },
                "output": {},
                "resources": {},
                "postCommit": {},
                "nodeSelector": {}
            },
            "status": {
                "lastVersion": 0
            }
        }
    ]
}
